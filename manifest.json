{
	"name": "CORS Bypass",
	"version": "1.0",
	"description": "Build an Extension!",
	"permissions": [
		"tabs",
		"webRequest",
		"storage",
		"<all_urls>"
	],
	"background": {
		"scripts": [
			"scripts/background.js"
		],
		"persistent": true
	},
	"page_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"manifest_version": 2,
	"devtools_page": "devtools.html"
}